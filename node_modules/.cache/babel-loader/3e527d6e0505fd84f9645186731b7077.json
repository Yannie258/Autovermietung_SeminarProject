AVY;AAWb,EAAA,OAAO,EAAE;AACP,IAAA,IADO,kBACC;AACN,WAAK,OAAL,CAAa,IAAb,CAAkB,QAAlB;AACD,KAHM;AAIN;AACD,IAAA,YALO,wBAKO,CALP,EAKU;AAAA;;AACf,MAAA,CAAC,CAAC,cAAF;AACA,UAAI,WAAU,GAAI,IAAI,MAAJ,CAAW,wBAAX,CAAlB;AACA,UAAI,QAAO,GAAI,IAAI,MAAJ,CAAW,iBAAX,CAAf;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AACA,UAAI,QAAO,GAAI,IAAI,MAAJ,CAAW,yDAAX,CAAf;AACA,UAAI,QAAO,GAAI,IAAI,MAAJ,CAAW,yDAAX,CAAf;;AACE,UAAI,KAAK,IAAL,CAAU,MAAV,GAAmB,CAAnB,IAAwB,KAAK,IAAL,CAAU,MAAV,GAAmB,GAA3C,IAAkD,QAAQ,CAAC,IAAT,CAAc,KAAK,IAAnB,CAAlD,IAA8E,KAAK,OAAL,CAAa,MAAb,GAAsB,CAApG,IAAyG,KAAK,OAAL,CAAa,MAAb,GAAsB,GAA/H,IAAsI,WAAW,CAAC,IAAZ,CAAiB,KAAK,OAAtB,CAA1I,EAA0K;AACzK,YAAI,KAAK,QAAL,CAAc,MAAd,GAAuB,CAAvB,IAA4B,KAAK,QAAL,CAAc,MAAd,GAAuB,GAAnD,IAA0D,QAAQ,CAAC,IAAT,CAAc,KAAK,QAAnB,CAA9D,EAA4F;AAC3F,cAAI,KAAK,QAAL,IAAiB,KAAK,qBAAtB,IAA+C,KAAK,QAAL,CAAc,MAAd,GAAuB,CAAtE,IAA2E,KAAK,QAAL,CAAc,MAAd,GAAuB,GAAtG,EAA2G;AACzG,gBAAI,QAAQ,CAAC,IAAT,CAAc,KAAK,QAAnB,CAAJ,EAAkC;AAChC,cAAA,IAAI,CAAC,gBAAL,CAAsB,KAAK,IAA3B,EAAiC,KAAK,OAAtC,EAA+C,KAAK,QAApD,EAA8D,KAAK,QAAnE,EACG,IADH,CACQ,UAAA,QAAO,EAAK;AAChB,gBAAA,KAAK,CAAC,QAAQ,CAAC,IAAV,CAAL;AACA,gBAAA,KAAI,CAAC,OAAL,GAAe,2BAAf;AACA,gBAAA,KAAI,CAAC,OAAL,GAAe,EAAf;AACA,gBAAA,KAAI,CAAC,IAAL,GAAY,EAAZ;AACA,gBAAA,KAAI,CAAC,QAAL,GAAgB,EAAhB;AACA,gBAAA,KAAI,CAAC,QAAL,GAAgB,EAAhB;AACA,gBAAA,KAAI,CAAC,qBAAL,GAA6B,EAA7B;AACD,eATH,EAUG,KAVH,CAUS,UAAC,KAAD;AAAA,uBAAW,MAAM,CAAC,MAAP,CAAc,KAAd,CAAX;AAAA,eAVT;AAWF,aAZA,MAYO;AACL,mBAAK,QAAL,GAAgB,EAAhB;AACA,mBAAK,qBAAL,GAA6B,EAA7B;AAEA,qBAAO,KAAK,CAAC,2BAAD,CAAZ;AACF;AACF,WAnBA,MAmBO;AACL,iBAAK,QAAL,GAAgB,EAAhB;AACA,iBAAK,qBAAL,GAA6B,EAA7B;AAEA,mBAAO,KAAK,CAAC,0BAAD,CAAZ;AACF;AACF,SA1BC,MA0BM;AACL,eAAK,QAAL,GAAgB,EAAhB;AACA,iBAAO,KAAK,CAAC,uEAAD,CAAZ;AACF;AACF,OA/BE,MA+BK;AACL,aAAK,IAAL,GAAY,EAAZ;AACA,aAAK,OAAL,GAAe,EAAf;AACA,eAAO,KAAK,CAAC,oDAAD,CAAZ;AACF;AACF;AAzDO;AAXI,CAAf","sourcesContent":["<template>\r\n    <div class=\"newEmployee\">\r\n        <h4>New Employee</h4>\r\n          <form>\r\n                <label for=\"vorname\">Vorname</label>\r\n                <div>\r\n                  <input id=\"vorname\" type=\"text\" v-model=\"vorname\" required autofocus>\r\n                </div>\r\n                <label for=\"name\">Name</label>\r\n                <div>\r\n                  <input id=\"name\" type=\"text\" v-model=\"name\" required autofocus>\r\n                </div>\r\n                <label for=\"username\">Username</label>\r\n                <div>\r\n                  <input id=\"username\" type=\"text\" v-model=\"username\" required autofocus>\r\n                </div>\r\n                \r\n\r\n                <label for=\"password\">Password</label>\r\n                <div>\r\n                  <input id=\"password\" type=\"password\" v-model=\"password\" required autofocus>\r\n                </div>\r\n                <label for=\"password-confirm\">Confirm Password</label>\r\n                <div>\r\n                    <input id=\"password-confirm\" type=\"password\" v-model=\"password_confirmation\" required autofocus>\r\n                </div>\r\n                <br/>\r\n            <div>\r\n                <button type=\"button\" @click=\"back\">\r\n                    Go back\r\n                </button>\r\n                <button type=\"submit\" @click=\"handleSubmit\">\r\n                    Create new Employee\r\n                </button>\r\n            </div>\r\n          </form>\r\n           <div>\r\n              <h3 >{{created}}</h3>\r\n           </div>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nimport Helper from '../services/helper.service'\r\nimport Auth from '../services/auth.service'\r\n\r\nexport default {\r\n  data () {\r\n    return {\r\n      vorname: '',\r\n      name: '',\r\n      username: '',\r\n      password: '',\r\n      password_confirmation: '',\r\n      created: '',\r\n    }\r\n  },\r\n  methods: {\r\n    back () {\r\n      this.$router.push('/admin')\r\n    },\r\n     //Registrieren neuer Mitarbeiter\r\n    handleSubmit (e) {\r\n      e.preventDefault()\r\n      var vornameTest = new RegExp('([a-zA-Z]{3,100}\\\\s*)+')\r\n      var nameTest = new RegExp('[a-zA-Z]{3,100}')\r\n      /* Regex: Strong Password\r\n      Special Characters - Not Allowed\r\n      Spaces - Not Allowed\r\n      Minimum and Maximum Length of field - 6 to 12 Characters\r\n      Met by [a-zA-Z0-9@]{6,12}\r\n      Numeric Character - At least one character\r\n      Met by positive lookahead (?=.*\\d)\r\n      At least one Capital Letter\r\n      Met by positive lookahead (?=.*[A-Z])\r\n      Repetitive Characters - Allowed only two repetitive characters */\r\n      var userTest = new RegExp('^(?=.*[A-Z])(?=.*\\\\d)(?!.*(.)\\\\1\\\\1)[a-zA-Z0-9@]{6,12}$')\r\n      var passTest = new RegExp('^(?=.*[A-Z])(?=.*\\\\d)(?!.*(.)\\\\1\\\\1)[a-zA-Z0-9@]{6,12}$')\r\n        if (this.name.length > 2 && this.name.length < 100 && nameTest.test(this.name) && this.vorname.length > 2 && this.vorname.length < 100 && vornameTest.test(this.vorname)) {\r\n         if (this.username.length > 5 && this.username.length < 100 && userTest.test(this.username)) {\r\n          if (this.password == this.password_confirmation && this.password.length > 0 && this.password.length < 100) {\r\n            if (passTest.test(this.password)) {\r\n              Auth.registerEmployee(this.name, this.vorname, this.username, this.password)\r\n                .then(response => {\r\n                  alert(response.data)\r\n                  this.created = 'User successfully created'\r\n                  this.vorname = ''\r\n                  this.name = ''\r\n                  this.username = ''\r\n                  this.password = ''\r\n                  this.password_confirmation = ''\r\n                })\r\n                .catch((error) => Helper.handle(error))\r\n            } else {\r\n              this.password = ''\r\n              this.password_confirmation = ''\r\n\r\n              return alert('Passwords not safe enough')\r\n            }\r\n          } else {\r\n            this.password = ''\r\n            this.password_confirmation = ''\r\n\r\n            return alert('Password is do not match')\r\n          }\r\n        } else {\r\n          this.username = ''\r\n          return alert('Username not save enough (6-12 Characters + 1x uppercase + 1x Number)')\r\n        }\r\n      } else {\r\n        this.name = ''\r\n        this.vorname = ''\r\n        return alert('Name too long or short or contains invalid symbols')\r\n      }\r\n    }\r\n  }\r\n\r\n}\r\n</script>\r\n<style  scoped>\r\n.newEmployee{\r\n  background:lightblue;\r\n  padding:10%\r\n  \r\n}\r\nh1 {\r\n  \r\n  font-weight:bold;\r\n  color: mediumblue;\r\n  padding:20px;\r\n  text-align: center;\r\n        \r\n  font-size: 30px;\r\n  }\r\ninput, select {\r\n  width: 30%;\r\n  padding: 12px 20px;\r\n  margin: 8px 0;\r\n  display: inline-block;\r\n  border: 1px solid #ccc;\r\n  border-radius: 4px;\r\n  box-sizing: border-box;\r\n}\r\nbutton{\r\n  box-shadow: 0px 0px 0px 2px #9fb4f2;\r\n\tbackground:linear-gradient(to bottom, #7892c2 5%, #476e9e 100%);\r\n\tbackground-color:#7892c2;\r\n\tborder-radius:10px;\r\n\tborder:1px solid #4e6096;\r\n\tdisplay:inline-block;\r\n\tcursor:pointer;\r\n\tcolor:#ffffff;\r\n\tfont-family:Arial;\r\n\tfont-size:14px;\r\n\tpadding:20px 37px 20px;\r\n\ttext-decoration:none;\r\n\ttext-shadow:0px 1px 0px #283966;\r\n  align-content: center;\r\n  width: 20%;\r\n}\r\nlabel{\r\n  color:indigo;\r\n}\r\nul {\r\n  list-style-type: none;\r\n   padding: 0;\r\n}\r\n\r\nli {\r\n  display: inline-block;\r\n  margin: 0 10px;\r\n}\r\na {\r\n  color: #42b983;\r\n}\r\ntable, th, td {\r\n  border: 1px solid black;\r\n  border-collapse: collapse;\r\n  padding: 15px;\r\n  border-spacing: 5px;\r\n  width: 100%;\r\n}\r\n</style>\r\n"],"sourceRoot":""}]}